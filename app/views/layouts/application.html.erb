<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
   <%= stylesheet_link_tag    "application", "ui-lightness/jquery-ui-1.9.1.custom.min.css",:media => "all" %>
  <%= javascript_include_tag "application", "jquery-1.8.2.js", "jquery-ui-1.9.1.custom.min.js","autocomplete-rails.js","rails.js" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
  <%= render 'layouts/header' %>
  <div class="container">
	<% flash.each do |key, value| %>
	    <div class="alert alert-<%= key %>"><%= value %></div>
	<% end %>	
	<%= yield %>
	<%= render 'layouts/footer' %>
        <%= debug(params) if Rails.env.development? %>
  </div>

</body>
</html>
